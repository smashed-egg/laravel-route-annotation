{
    "name": "smashed-egg/laravel-route-annotation",
    "description": "Adds support for Route Annotations in Laravel",
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^9.21|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^9.21|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.21|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0|^12.0"

    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SmashedEgg\\LaravelRouteAnnotation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmashedEgg\\LaravelRouteAnnotation\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tom Ellis"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "SmashedEgg\\LaravelRouteAnnotation\\RouteAnnotationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
